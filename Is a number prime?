def prime_number(n):

 if n == 2:
  return True

 if n > 1:
  for i in range(2, n):
   if (n % i) == 0:
    return False
   return True
 else:
    return False



number = int(input("Enter any number: "))

if prime_number(number):
  print("It is a prime number")
else:
  print("It is not a prime number")
